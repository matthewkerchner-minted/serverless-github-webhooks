    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/handler.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "getJiraIssue",
            "program": "${workspaceFolder}/node_modules/.bin/sls",
            "args": [
                "invoke",
                "local",
                "-f",
                "getJiraIssue",
                "-d",
                "{\"body\":{\"key\":\"NPD-655\"}}"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "githubWebhookListener",
            "program": "${workspaceFolder}/node_modules/.bin/sls",
            "args": [
                "invoke",
                "local",
                "-f",
                "githubWebhookListener",
                "-p",
                "${workspaceFolder}/test/githubPullRequest.json"
            ]
        }
    ]
}